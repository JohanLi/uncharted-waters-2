{
  "name": "uncharter-waters-2",
  "version": "1.0.0",
  "description": "Remaking Uncharted Waters 2 using web technologies",
  "scripts": {
    "start": "webpack-dev-server --open",
    "lint": "eslint app/**/*.{js,jsx}",
    "setup": "ansible-playbook -i ansible/hosts ansible/setup.yml",
    "build": "webpack --profile -p",
    "deploy": "rsync -a public/ root@johanli.com:/var/www/unchartedwaters2.com"
  },
  "author": "Johan Li",
  "license": "MIT",
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "airbnb",
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ]
    }
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "node-sass": "^4.3.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-server": "^2.2.0-rc.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "whatwg-fetch": "^2.0.3"
  }
}
