{
  "name": "uncharter-waters-2",
  "version": "1.0.0",
  "description": "Remaking Uncharted Waters 2 using web technologies",
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "lint": "eslint app/**/*.{js,jsx}",
    "setup": "ansible-playbook -i ansible/hosts ansible/setup.yml",
    "build": "rm -rf dist && webpack --profile -p --mode production",
    "deploy": "rsync -a public/ root@johanli.com:/var/www/unchartedwaters2.com"
  },
  "author": "Johan Li",
  "license": "MIT",
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "airbnb",
    "rules": {
      "comma-dangle": [
        "error",
        "never"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^9.4.7",
    "@types/react": "^16.0.25",
    "@types/react-dom": "^16.0.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "mini-css-extract-plugin": "^0.2.0",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "ts-loader": "^4.0.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "mobx": "^4.1.0",
    "mobx-react": "^5.0.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "whatwg-fetch": "^2.0.3"
  }
}
